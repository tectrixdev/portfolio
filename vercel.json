{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "arcade.tectrix.dev"
        }
      ],
      "destination": "https://www.tectrix.dev/blog/arcade"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "blog.tectrix.dev"
        }
      ],
      "destination": "https://www.tectrix.dev/blog/:path*"
    }
  ]
}
